        <footer-portlet-javascript>/js/fancyapps-fancyBox/source/jquery.fancybox.pack.js</footer-portlet-javascript> 
	    <footer-portlet-javascript>/js/jquery.json-2.3/jquery.json-2.3.min.js</footer-portlet-javascript>
	    <footer-portlet-javascript>/js/google_analytics/analyticsChartDataEncoder.js</footer-portlet-javascript>
		<footer-portlet-javascript>/js/google_analytics/chartGraphicator.js</footer-portlet-javascript>
