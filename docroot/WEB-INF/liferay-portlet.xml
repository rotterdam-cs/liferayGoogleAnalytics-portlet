		<icon>/google-analytics-icon.png</icon>
		<instanceable>false</instanceable>
		<requires-namespaced-parameters>false</requires-namespaced-parameters>
		<header-portlet-css>/js/jquery-validation-1.9.0/demo/css/screen.css</header-portlet-css>
        <header-portlet-css>/js/jquery-loadmask/jquery.loadmask.css</header-portlet-css>
        <header-portlet-css>/js/fancyapps-fancyBox/source/jquery.fancybox.css</header-portlet-css>        
        <header-portlet-css>/js/google_analytics/analytics_dashboard.css</header-portlet-css>
        <header-portlet-css>/js/rangeDatePicker/css/datepicker/base.css</header-portlet-css>
        <header-portlet-css>/js/rangeDatePicker/css/datepicker/clean.css</header-portlet-css>
		<header-portlet-css>/css/main.css</header-portlet-css>

